{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-console": [0],
    "indent": [2, 2, {"SwitchCase": 1}],
    "semi": [2, "never"],
    "no-extra-semi": 2,
    "no-unexpected-multiline": 2,
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "semi-spacing": 2,
    "space-before-function-paren": ["error", {"anonymous": "never", "named": "never"}]
  }
}
